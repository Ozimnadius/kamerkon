extends ../src/core/main

block variables
    - var title = 'Кастинг молодых исполнителей';
    - var bgi = 'background-image:url(/images/content/bg/gallery.jpg);'

block content
    .page.casting
        .container
            .casting__title.title Кастинг<br>молодых исполнителей
            form(action="/php/ajax.php" method="post" enctype="multipart/form-data" data-event='sendCasting').casting__form
                .casting__fields
                    .casting__field
                        .casting__line
                        input(type='text' name='name' placeholder='Ваше имя').casting__input
                    .casting__field
                        .casting__line
                        input(type='tel' name='tel' placeholder='*Номер телефона' required).casting__input
                    .casting__field
                        .casting__line
                        input(type='email' name='email' placeholder='Эл. почта').casting__input
                    .casting__field
                        .casting__line
                        input(type='text' name='yt' placeholder='Ссылка на видео(youtube)').casting__input
                    .casting__field
                        .casting__line
                        label.casting-file
                            input(type='file' name='resume' placeholder='Закрепить резюме в формате .docx' accept=".doc,.docx,application/msword").casting-file__input
                            span.casting-file__fake Закрепить резюме в формате .docx
                        .casting__line

                .casting__accept
                    include ../src/components/accept/index
                button(type='submit').casting__submit
